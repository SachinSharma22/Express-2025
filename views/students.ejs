<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <h1>Data views here</h1>
    <table border="1">
      <thead>
        <tr>
          <th>Name</th>
          <th>age</th>
          <th>email</th>
          <th>Operation</th>
        </tr>
      </thead>
      <tbody>
        <% students.forEach(student => {%>
        <tr>
          <td><%=student.name%></td>
          <td><%=student.age%></td>
          <td><%=student.email%></td>
          <td><a href="/ui/delete/<%= student._id %>">Delete</a></td>
        </tr>
        <%})%>
      </tbody>
    </table>
  </body>
</html>
